@{
    var source = HttpContext.Current.Request.RequestContext.RouteData.Values["Controller"].ToString();
    var layout = "";
    if(source == "Home")
    {
        layout = "~/Views/Shared/_Layout.cshtml";
    }
    else
    {
        layout = "~/Views/Shared/_AdminLayout.cshtml";
    }
    Layout = layout;
}
